{
  "name": "integer-net/solr-pro",
  "description": "Additional libraries for IntegerNet_Solr commercial version",
  "type": "magento-module",
  "require": {
    "integer-net/solr-base": "dev-master"
  },
  "require-dev": {
    "psr/cache": "1.0.0",
    "katzgrau/klogger": "~1.0",
    "mikey179/vfsStream": "~1.3",
    "phpunit/phpunit": "~4.5",
    "phpunit/php-invoker": "~1.1"
  },
  "autoload": {
    "psr-4": {
      "IntegerNet\\SolrCategories\\": "src/SolrCategories/",
      "IntegerNet\\SolrSuggest\\": "src/SolrSuggest/",
      "Katzgrau\\KLogger\\": "lib/KLogger/src/",
      "Psr\\Cache\\": "lib/Psr/Cache/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IntegerNet\\Solr\\": "vendor/integer-net/solr-base/test/Solr/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:integer-net/solr-base.git"
    }
  ],
  "authors": [
    {
      "name": "Andreas von Studnitz",
      "email": "avs@integer-net.de"
    },
    {
      "name": "Fabian Schmengler",
      "email": "fs@integer-net.de"
    }
  ]
}
